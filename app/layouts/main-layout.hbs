<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  {{#if pageDescription}}
  <meta name="Description" content="{{{pageDescription}}}">
  {{/if}}
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{pageTitle}}</title>
  <link rel="shortcut icon" type="image/png" href="images/favicon2.png" />
  <style>
{{#minify}}
body {
  	font-family: 'Open Sans', sans-serif; 
  	padding: 0px;
  	margin: 0px;
}
#banner {
  	background: #0070BA;
  	color: white;
  	position: fixed;
  	width: 100%;
  	height: 130px;
  	top: 0px;
    z-index: 50;
}
.menu-text-bar-wrapper {
  	position: absolute;
  	top: 106px;
  	width: 100%;
}
.menu-text-bar {
  	text-align: center;
  	vertical-align: top;
  	background: #B0C8EA;
}
#main-nav {
	display: none;
}
{{/minify}}
  </style>
</head>
{{#minify}}
<body>
	<div id="banner">
		<div class="banner-logo logo"></div>
		<div class="banner-logo dance-scottish-logo"></div>
		<div class="menu-text-bar-wrapper">
			<div class="menu-text-bar">					

				<div id="main-nav" role="navigation">
				
					<!-- Mobile menu toggle button (hamburger/x icon) -->
					<input id="main-menu-state" type="checkbox" />
					<label class="main-menu-btn" for="main-menu-state">
					  <span class="main-menu-btn-icon"></span> Toggle main menu visibility
					</label>
		
					<ul id="main-menu" class="sm sm-mint">
					{{#each menus}}
						<li>{{#if page}} <a href="{{page}}.html">{{name}}</a>
							{{/if}}
							{{#if items}}
							<a href="#">{{name}}</a>
							<ul>
							{{#each items}}
								<li><a href="{{page}}">{{name}}</a>
								</li>
							{{/each}}
							</ul>
							{{/if}}
					{{/each}}
					</ul>
				</div>

			</div>
		</div>
	</div>
	
	
	

<div style="height: 140px">
</div>

{{> body  }} 

<link rel="stylesheet" href="stylesheets/site.css?{{ fileHash 'dist/stylesheets/site.css' 10 }}">
<link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="vendor/smartmenu.css">
  
<script src="vendor/jquery-1.12.3.min.js"></script>

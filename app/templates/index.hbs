---
pageTitle: "RSCDS Leeds Branch"
scripts: ["eventLinks();",
          "handleEmailLinks();"]
eventLinks:
   - date: 19th Mar
     name: Branch Dance
   - date: 21st May
     name: Branch Dance
   - date: 18th Jun
     name: Branch Dance
leedsEventLinks:
   - date: 12th Mar
     name: Club Dance
   - date: 9th Apr
     name: Club Dance
   - date: 28th Dec
     name: St Columba
specialEventLinks:
   - date: 19th Mar 
     name: Children's Festival
   - date: 1st - 3rd Apr
     name: Scarborough Weekend
   - date: 23rd Apr
     name: Highland and Scottish National Day Workshop 
pageDescription: The Leeds branch of the Royal Scottish Country Dance Society.
---

<div id="upcoming-events">
	<table>
		<tr>
			<td id="upcoming-branch-events">
				<h2>Branch Events</h2>
				<div class="light-heading-panel">
					<table class="tabular summary">
					  <colgroup>
						<col class="col1">
						<col class="col2">
					  </colgroup>
						<tbody>
						{{#each eventLinks}}
							<tr>
								<td>{{date}}</td>
								<td>
								{{#if name}}<a href="events.html">{{name}}</a>{{/if}}
								{{#if cancelled}}<div class="cancelled">{{cancelled}}</div>{{/if}}
								</td>
							</tr>
						{{/each}}
						</tbody>
					</table>
				</div>
			</td>
			<td id="upcoming-other-events">
				<h2>Leeds Events</h2>
				<div class="light-heading-panel">
					<table class="tabular summary">
					  <colgroup>
						<col class="col1">
						<col class="col2">
					  </colgroup>
					{{#each leedsEventLinks}}
						<tr>
							<td>{{date}}</td>
							<td><a href="leeds_events.html">{{name}}</a></td>
						</tr>
					{{/each}}
					</table>
				</div>
			</td>			
			<td id="upcoming-special-events">
				<h2>Special Branch Events</h2>
				<div class="light-heading-panel">
					<table class="tabular summary">
					  <colgroup>
						<col class="col1">
						<col class="col2">
					  </colgroup>
					{{#each specialEventLinks}}
						<tr>
							<td>{{date}}</td>
							<td><a href="special_events.html">{{name}}</a></td>
						</tr>
					{{/each}}
					</table>
				</div>
			</td>
		</tr>

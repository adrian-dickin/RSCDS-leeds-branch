<ul class="list-group">
{{#each data-classes.classes}}
	<li class="list-group-item">
		<div class="row">
			<div class="col-md-3 col-xs-6">
				{{day}}: {{time}}
			</div>
			<div class="col-md-3 col-xs-6">
				<div>{{level}}</div>
				{{#each teacher}}
					<div><a href="branch_members.html#teachers">{{this}}</a></div>
				{{/each}}
			</div>
			<div class="col-md-3 col-xs-6">
				{{#each venue}}
					<div>{{this}}</div>
				{{/each}}
			</div>
			<div class="col-md-3 col-xs-6">
				{{#each notes}}
				<div>{{this}}</div>
				{{/each}}
			</div>
		</div>
	</li>
{{/each}}
</ul>
